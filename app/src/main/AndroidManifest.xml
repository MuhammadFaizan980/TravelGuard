<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whatsclone.muhammadfaizan.travelguard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Signin_Register.View.LoginView"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Signin_Register.View.RegistrationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashScreen.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainScreen.MainActivity" />
        <activity android:name=".EditUserProfile.View.ActivityEditUserProfile"></activity>
    </application>

</manifest>